      MENUITEM "$$ABOUT..." mitAbout "A"
      MENUITEM "$$HELP" hlpMain2 "H"
      MENUITEM "$$REFERENCE1" hlpFunc 
      MENUITEM SEPARATOR
      MENUITEM "Thanks to..." mitCredits
    END
